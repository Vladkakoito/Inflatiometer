{
  "configurations": [
    {
      "type": "cppdbg",
      "request": "launch",
      "environment": [],
      "externalConsole": false,
      "cwd": "${fileDirname}",
      "MIMode": "gdb",
      "name": "CController",
      "program": "${workspaceFolder}/Bin/CController",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}